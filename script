local G2L = {}

if not isfolder("Koronis") then
	makefolder("Koronis")
end
KeyModule = loadstring(game:HttpGet("https://raw.githubusercontent.com/nf-36/Koronis/refs/heads/main/Data/KeySystemModule.lua"))()

local keyFile
if not isfile("Koronis/key.txt") then
	writefile("Koronis/key.txt","")
	keyFile = readfile("Koronis/key.txt")
else
	keyFile = readfile("Koronis/key.txt")
	local response = KeyModule.Functions.CheckKey(keyFile)
	if response.STATUS.code == "KEY_VALID" then
		-- Loading screen GUI
		local loadingGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
		loadingGui.Name = "LoadingUI"

		local frame = Instance.new("Frame", loadingGui)
		frame.Size = UDim2.new(0, 300, 0, 150)
		frame.Position = UDim2.new(0.5, -150, 0.5, -75)
		frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
		frame.BorderSizePixel = 0

		local title = Instance.new("TextLabel", frame)
		title.Text = "SKIPPING TO 100%"
		title.Size = UDim2.new(1, 0, 0.2, 0)
		title.Position = UDim2.new(0, 0, 0, 0)
		title.TextColor3 = Color3.fromRGB(0, 255, 255)
		title.BackgroundTransparency = 1
		title.Font = Enum.Font.GothamBold
		title.TextScaled = true

		local subtitle = Instance.new("TextLabel", frame)
		subtitle.Text = "SCRIPT LOADING"
		subtitle.Size = UDim2.new(1, 0, 0.2, 0)
		subtitle.Position = UDim2.new(0, 0, 0.2, 0)
		subtitle.TextColor3 = Color3.fromRGB(255, 0, 255)
		subtitle.BackgroundTransparency = 1
		subtitle.Font = Enum.Font.GothamBold
		subtitle.TextScaled = true

		local progressBarBack = Instance.new("Frame", frame)
		progressBarBack.Size = UDim2.new(0.9, 0, 0.2, 0)
		progressBarBack.Position = UDim2.new(0.05, 0, 0.5, 0)
		progressBarBack.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		progressBarBack.BorderSizePixel = 0

		local progressBar = Instance.new("Frame", progressBarBack)
		progressBar.Size = UDim2.new(1, 0, 1, 0)
		progressBar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
		progressBar.BorderSizePixel = 0

		local credit = Instance.new("TextLabel", frame)
		credit.Text = "Made by MilesMTG and KryptKid"
		credit.Size = UDim2.new(1, 0, 0.2, 0)
		credit.Position = UDim2.new(0, 0, 0.75, 0)
		credit.TextColor3 = Color3.fromRGB(255, 255, 255)
		credit.BackgroundTransparency = 1
		credit.Font = Enum.Font.Gotham
		credit.TextScaled = true

		wait(3)
		loadingGui:Destroy()

		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "Found User",
			Text = "Key Found, Loading Koronis...",
			Duration = 5
		})
		script_key = keyFile
		response.API.load_script()
		script:Destroy()
		return
	end
end

local kSpoof = game:GetService("HttpService"):GenerateGUID(false)
local KORONIS_SAVEKEY = false

