-- Load key system GUI (injected below this line automatically)
local function C_25()
	local G2L_UI, requireUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/nf-36/Koronis/refs/heads/main/KeyUI/Gui.lua"))()
	task.spawn(C_25)
	return G2L_UI, requireUI;
end;

return C_25();
